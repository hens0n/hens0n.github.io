&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> &lt;span style="color:#66d9ef">for&lt;/span>(map_entry in item.&lt;span style="color:#a6e22e">entrySet&lt;/span>()){
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> temp.&lt;span style="color:#a6e22e">add&lt;/span>(&lt;span style="color:#f92672">[&lt;/span>map_entry.&lt;span style="color:#a6e22e">getKey&lt;/span>().&lt;span style="color:#a6e22e">toLowerCase&lt;/span>():map_entry.&lt;span style="color:#a6e22e">getValue&lt;/span>()&lt;span style="color:#f92672">]&lt;/span>)
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> }
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> lower_array.&lt;span style="color:#a6e22e">add&lt;/span>(temp);
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> }
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> ctx.&lt;span style="color:#a6e22e">field_name&lt;/span> &lt;span style="color:#f92672">=&lt;/span> lower_array;
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>}
